							}}
						}}
					},



					{
						"condition": {"QualifiedDomain": "l.threads.com"},
						"mapper": {"GetUrlFromQueryParam": "u"}
					},



          {
						"condition": {"All": [
							{"UnqualifiedAnyTld": "google"},
							{"PathIs": "/url"}
						]},
						"mapper": {"GetUrlFromQueryParam": "q"}
					},



