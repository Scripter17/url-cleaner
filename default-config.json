		},
		{
			"comment": "Verify weird inputs cause no issues",
			"pairs": [
				["https://127.0.0.1", "https://127.0.0.1"],
				["https://com", "https://com"],
				["ftp://127.0.0.1", "ftp://127.0.0.1"],
				["file://a/b/c", "file://a/b/c"]
