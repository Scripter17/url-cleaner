						"condition": {"All": [
							{"FlagIsSet": "bypass.vip"},
							{"HostIsOneOf": [
								"adf.ly", "adfoc.us", "ay.live", "aylink.co", "bc.vc", "bcvc.live",
								"boost.fusedgt.com", "boost.ink", "boostme.link", "cutt.ly", "eio.io", "exe.app", "exe.io",
